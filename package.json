		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"nuxt": "^3.7.4",
		"vue": "^3.3.4",
		"vue-router": "^4.2.5"
	},
	"dependencies": {
		"@nuxtjs/eslint-config-typescript": "^12.1.0"
	}
}
