		"electron": "^27.1.0",
		"electron-builder": "^24.6.4",
		"element-plus": "^2.3.14",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"nuxt": "^3.7.4",
